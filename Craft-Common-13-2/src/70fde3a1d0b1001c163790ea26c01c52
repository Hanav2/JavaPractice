package com.day7.AcModifier;

public class Constractor { // constractor is 
	// the method name must be similar to the class name
	// Constractor will help to initialize the value at the time of object creation. objectu create betederegebet seat value initialize enaregalen.
	// objectu value yezo creat yederegal
	// we give value one time when we create a value 
	int age;
	int salary;
	
	public Constractor (int a, int s) {
		this.age = a;  // here we are assigning age for a
		this.salary =s;
	}
	public Constractor (int b) {
		this.age = b;  // here we are assigning age for a
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Constractor c= new Constractor(12, 17000);  //we don't need to do c. menamen
		System.out.println(c.age);
		System.out.println(c.salary);
		
		Constractor d = new Constractor (25);
		System.out.println(d.age);
	}

}
